{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "sourceMap": true,
    "jsx": "react",
    "outDir": "build",
    "lib": ["dom", "es2015"]
  },
  "exclude": ["cypress"]
}
