{
  "name": "typescript-react-redux",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "Drew Schrauf",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dashboard -m -- webpack-dev-server --host 0.0.0.0 --quiet",
    "build": "rm -rf dist && webpack --config webpack-prod.config.js",
    "lint": "eslint --ext ts,tsx src",
    "test": "jest",
    "test:cover": "jest --coverage --coverageReporters html",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "stats": "yarn build && yarn stats:analyze && yarn stats:start",
    "stats:analyze": "webpack --config webpack-prod.config.js --profile --json > dist/stats.json",
    "stats:start": "webpack-bundle-analyzer dist/stats.json --host 0.0.0.0",
    "ci": "yarn lint && yarn test:coveralls && yarn build"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "23.3.10",
    "@types/lodash": "4.14.119",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "6.0.10",
    "@types/redux-mock-store": "1.0.0",
    "@types/styled-components": "4.1.3",
    "coveralls": "3.0.2",
    "css-loader": "1.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "mini-css-extract-plugin": "0.4.5",
    "prettier": "1.15.3",
    "react-addons-test-utils": "15.6.2",
    "redux-mock-store": "1.5.3",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.1",
    "typescript": "3.2.1",
    "typescript-eslint-parser": "21.0.2",
    "typescript-plugin-styled-components": "1.0.0",
    "webpack": "4.27.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {
    "lodash": "4.17.11",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "styled-components": "4.1.2",
    "tslib": "1.9.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  }
}
