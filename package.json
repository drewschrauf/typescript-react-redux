{
  "name": "typescript-react-redux",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "Drew Schrauf",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --hot",
    "build": "rimraf build && cross-env NODE_ENV=production webpack && cpy _redirects build",
    "lint": "yarn lint:eslint && yarn lint:types",
    "lint:eslint": "eslint --ext ts,tsx src ",
    "lint:types": "tsc",
    "test": "jest",
    "test:mutate": "stryker run",
    "test:coveralls": "jest --verbose --coverage --coverageReporters=text-lcov | coveralls",
    "stats": "yarn stats:analyze && yarn stats:start",
    "stats:analyze": "cross-env NODE_ENV=production webpack --profile --json > build/stats.json",
    "stats:start": "webpack-bundle-analyzer build/stats.json --host 0.0.0.0",
    "ci": "yarn lint && yarn test:coveralls && yarn build"
  },
  "dependencies": {
    "core-js": "3.17.2",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-is": "17.0.2",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.15.4",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@hot-loader/react-dom": "17.0.1",
    "@stryker-mutator/core": "5.4.0",
    "@stryker-mutator/jest-runner": "5.4.0",
    "@stryker-mutator/typescript": "4.0.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/jest": "27.0.1",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.3.1",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "@vanilla-extract/babel-plugin": "^1.0.1",
    "@vanilla-extract/css": "^1.4.0",
    "@vanilla-extract/webpack-plugin": "^2.0.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "coveralls": "3.1.1",
    "cpy-cli": "3.1.1",
    "cross-env": "7.0.3",
    "css-loader": "6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.9.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.1.0",
    "markdown-loader": "6.0.0",
    "mini-css-extract-plugin": "2.2.2",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "style-loader": "3.2.1",
    "terser-webpack-plugin": "5.2.3",
    "typescript": "4.4.2",
    "webpack": "5.52.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.1.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/*.test.(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.css$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-test.ts"
    ],
    "coveragePathIgnorePatterns": [
      "setup-test.ts",
      ".*/__tests__/.*"
    ]
  }
}
