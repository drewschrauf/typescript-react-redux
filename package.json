{
  "name": "typescript-react-redux",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "Drew Schrauf",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack && cp _redirects dist",
    "lint": "eslint --ext ts,tsx src && yarn check-types",
    "check-types": "tsc",
    "test": "jest",
    "test:coveralls": "jest --verbose --coverage --coverageReporters=text-lcov | coveralls",
    "stats": "yarn stats:analyze && yarn stats:start",
    "stats:analyze": "cross-env NODE_ENV=production webpack --profile --json > dist/stats.json",
    "stats:start": "webpack-bundle-analyzer dist/stats.json --host 0.0.0.0",
    "ci": "yarn lint && yarn test:coveralls && yarn build"
  },
  "dependencies": {
    "core-js": "3.0.1",
    "lodash": "4.17.11",
    "normalize.css": "8.0.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.12",
    "@types/lodash": "4.14.126",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.0.8",
    "@types/react-router-dom": "4.3.3",
    "@types/styled-components": "4.1.8",
    "@typescript-eslint/parser": "1.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.0",
    "coveralls": "3.0.3",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-dom": "3.2.2",
    "markdown-loader": "5.0.0",
    "mini-css-extract-plugin": "0.6.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prettier": "1.17.1",
    "react-testing-library": "7.0.0",
    "terser-webpack-plugin": "1.2.3",
    "typescript": "3.4.5",
    "webpack": "4.31.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.css$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-test.ts"
    ],
    "coveragePathIgnorePatterns": [
      "setup-test.ts"
    ]
  }
}
