{
  "name": "typescript-react-redux",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "Drew Schrauf",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "build": "rm -rf dist && NODE_ENV=production webpack --config webpack-prod.config.js && cp _redirects dist",
    "lint": "eslint --ext ts,tsx src && yarn check-types",
    "check-types": "tsc",
    "test": "jest",
    "test:cover": "jest --coverage --coverageReporters html",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "stats": "yarn build && yarn stats:analyze && yarn stats:start",
    "stats:analyze": "webpack --config webpack-prod.config.js --profile --json > dist/stats.json",
    "stats:start": "webpack-bundle-analyzer dist/stats.json --host 0.0.0.0",
    "ci": "yarn lint && yarn test:coveralls && yarn build"
  },
  "dependencies": {
    "core-js": "2.6.4",
    "lodash": "4.17.11",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/enzyme": "3.1.16",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "24.0.0",
    "@types/lodash": "4.14.120",
    "@types/react": "16.8.1",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "7.0.1",
    "@types/react-router-dom": "4.3.1",
    "@types/redux-mock-store": "1.0.0",
    "@types/styled-components": "4.1.8",
    "@typescript-eslint/parser": "1.2.0",
    "babel-loader": "8.0.5",
    "babel-plugin-styled-components": "1.10.0",
    "coveralls": "3.0.2",
    "css-loader": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
    "markdown-loader": "5.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "prettier": "1.16.4",
    "react-addons-test-utils": "15.6.2",
    "redux-mock-store": "1.5.3",
    "typescript": "3.3.1",
    "webpack": "4.29.2",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/setup-test.ts"
    ],
    "coveragePathIgnorePatterns": [
      "setup-test.ts",
      "testHook.ts"
    ]
  }
}
