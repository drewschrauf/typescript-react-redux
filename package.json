{
  "name": "typescript-react-redux",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "Drew Schrauf",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --hot",
    "build": "rimraf build && cross-env NODE_ENV=production webpack && cpy _redirects build",
    "lint": "eslint --ext ts,tsx src && yarn check-types",
    "check-types": "tsc",
    "test": "jest",
    "test:coveralls": "jest --verbose --coverage --coverageReporters=text-lcov | coveralls",
    "stats": "yarn stats:analyze && yarn stats:start",
    "stats:analyze": "cross-env NODE_ENV=production webpack --profile --json > build/stats.json",
    "stats:start": "webpack-bundle-analyzer build/stats.json --host 0.0.0.0",
    "ci": "yarn lint && yarn test:coveralls && yarn build"
  },
  "dependencies": {
    "core-js": "3.3.5",
    "lodash": "4.17.15",
    "normalize.css": "8.0.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-hot-loader": "4.12.15",
    "react-router-dom": "5.1.2",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@hot-loader/react-dom": "16.10.2",
    "@testing-library/jest-dom": "4.2.2",
    "@testing-library/react": "9.3.1",
    "@types/jest": "24.0.19",
    "@types/lodash": "4.14.144",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-router-dom": "5.1.1",
    "@types/styled-components": "4.1.20",
    "@typescript-eslint/parser": "2.6.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "coveralls": "3.0.7",
    "cpy-cli": "2.0.0",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "error-overlay-webpack-plugin": "0.4.1",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "markdown-loader": "5.1.0",
    "mini-css-extract-plugin": "0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "2.2.1",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.css$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-test.ts"
    ],
    "coveragePathIgnorePatterns": [
      "setup-test.ts"
    ]
  }
}
